{
  "name": "thamanyanls",
  "version": "0.1.0",
  "description": "Context-aware layout system for deep interfaces",
  "main": "dist/thamanyan.css",
  "style": "dist/thamanyan.css",
  "unpkg": "dist/thamanyan.js",
  "exports": {
    ".": {
      "style": "./dist/thamanyan.css",
      "default": "./dist/thamanyan.css"
    },
    "./dist/thamanyan.css": "./dist/thamanyan.css",
    "./dist/thamanyan.js": "./dist/thamanyan.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "node -e \"const fs=require('fs'); fs.mkdirSync('dist',{recursive:true}); fs.copyFileSync('src/thamanyan.css','dist/thamanyan.css'); fs.copyFileSync('src/thamanyan.js','dist/thamanyan.js');\"",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "css",
    "layout",
    "flexbox",
    "spacing",
    "nested"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/veertoooz/ThamanyanLS.git"
  },
  "homepage": "https://github.com/veertoooz/ThamanyanLS#readme",
  "bugs": {
    "url": "https://github.com/veertoooz/ThamanyanLS/issues"
  },
  "author": "veertoooz"
}
